{
  "name": "gutsy-reviews",
  "version": "1.0.0",
  "description": "The reviews microservice for the Gutsy app",
  "main": "index.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "drop": "knex migrate:rollback",
    "reset": "npm run drop && npm run migrate",
    "seed": "knex seed:run",
    "react-prod": "webpack -p --config webpack.prod.js",
    "react-dev": "webpack -d --watch --display-error-details --config webpack.dev.js",
    "server-dev": "nodemon server/index.js",
    "test": "jest",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "update": "pm2 kill && pm2 start ecosystem.config.js --env production",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "build": "webpack --config webpack.prod.js",
    "db-start": "pg_ctl -D /usr/local/var/postgres start",
    "db-stop": "pg_ctl -D /usr/local/var/postgres stop",
    "db": "psql btsi-reviews"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btsi/gutsy-reviews.git"
  },
  "author": "James Boyett and Scott Elder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/btsi/gutsy-reviews/issues"
  },
  "homepage": "https://github.com/btsi/gutsy-reviews#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-flow": "^6.23.0",
    "bluebird": "^3.5.3",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "knex": "^0.16.3",
    "moment": "^2.24.0",
    "pg": "^7.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-star-ratings": "^2.3.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.2",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "artillery": "^1.6.0-27",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.2.9",
    "prettier-eslint": "^8.8.2"
  }
}
